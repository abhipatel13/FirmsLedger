{
    "name": "Lead",
    "type": "object",
    "properties": {
      "user_id": {
        "type": "string",
        "description": "Buyer user ID"
      },
      "company_name": {
        "type": "string",
        "description": "Company requesting staffing"
      },
      "contact_email": {
        "type": "string",
        "description": "Contact email"
      },
      "contact_phone": {
        "type": "string",
        "description": "Contact phone"
      },
      "role": {
        "type": "string",
        "description": "Role to be hired"
      },
      "number_of_hires": {
        "type": "number",
        "description": "Number of positions"
      },
      "budget_range": {
        "type": "string",
        "enum": [
          "Under $50k",
          "$50k-$100k",
          "$100k-$200k",
          "$200k-$500k",
          "$500k+",
          "Not specified"
        ],
        "description": "Budget range"
      },
      "timeline": {
        "type": "string",
        "enum": [
          "Immediate (< 1 month)",
          "1-3 months",
          "3-6 months",
          "6+ months",
          "Flexible"
        ],
        "description": "Hiring timeline"
      },
      "country": {
        "type": "string",
        "description": "Country"
      },
      "state": {
        "type": "string",
        "description": "State/Province"
      },
      "city": {
        "type": "string",
        "description": "City"
      },
      "remote_allowed": {
        "type": "boolean",
        "default": false,
        "description": "Remote work allowed"
      },
      "description": {
        "type": "string",
        "description": "Detailed requirements"
      },
      "status": {
        "type": "string",
        "enum": [
          "Open",
          "In Progress",
          "Closed",
          "Cancelled"
        ],
        "default": "Open",
        "description": "Lead status"
      },
      "agencies_contacted": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "description": "Agency IDs that received this lead"
      }
    },
    "required": [
      "role",
      "number_of_hires",
      "timeline"
    ]
  }